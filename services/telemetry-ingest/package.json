{
  "name": "@syntra/telemetry-ingest",
  "version": "0.1.0",
  "private": true,
  "description": "Syntra Telemetry Ingest Service - Redis to ClickHouse pipeline",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch --onSuccess 'node dist/index.js'",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "lint": "eslint src --ext .ts",
    "test": "vitest",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "@clickhouse/client": "^0.2.0",
    "ioredis": "^5.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0"
  }
}
